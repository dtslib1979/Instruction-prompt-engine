{
  "name": "instruction-prompt-engine",
  "version": "24.1.0",
  "description": "A Progressive Web App for managing and organizing AI prompts and instructions",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js",
    "lint:fix": "eslint . --ext .ts,.tsx,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "node tests/smoke.test.js",
    "diagnose": "bash scripts/repo_diagnose.sh",
    "serve": "npx serve -s . -l 8080",
    "dev": "npx serve -s . -l 8080"
  },
  "keywords": [
    "pwa",
    "prompt",
    "ai",
    "instruction",
    "github-copilot"
  ],
  "author": "dtslib1979",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  }
}
